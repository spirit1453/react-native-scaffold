{
  "name": "<%=appName%>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run start:ios",
    "start:ios": "node bin/startIOS",
    "start:android": "react-native run-android",
    "xcode": "xed ios",
    "packager": "react-native start",
    "lint": "eslint --fix --ext .js .",
    "showDevMenu": "adb shell input keyevent 82",
    "pack:ios": "node bin/packIOS",
    "pack:ppk": "node bin/packPPK",
    "pack:android": "node bin/packAndroid",
    "pack:all": "npm run pack:ppk && npm run pack:android && npm run pack:ios",
    "jsBundle": " react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "updateSubmodule": "git submodule update --remote && git submodule update",
    "init": "git submodule init && git submodule update --remote && yarn",
    "test": "jest"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "native-base": "^2.13.5",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-camera": "^3.3.0",
    "react-native-communications": "^2.2.1",
    "react-native-device-info": "^2.3.2",
    "react-native-elements": "^1.1.0",
    "react-native-fs": "^2.14.1",
    "react-native-image-picker": "^1.0.2",
    "react-native-image-zoom-viewer": "^2.2.26",
    "react-native-qrcode-svg": "^5.1.2",
    "react-native-restart": "^0.0.12",
    "react-native-sqlite-storage": "^4.0.0",
    "react-native-update": "^5.2.3",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.12.1",
    "react-navigation-backhandler": "^1.3.2",
    "react-redux": "^7.1.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "chalk": "^2.4.2",
    "eslint": "^6.2.2",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
